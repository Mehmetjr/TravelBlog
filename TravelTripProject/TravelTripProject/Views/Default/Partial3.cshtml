@using TravelTripProject.Models.Classes
@model List<Blog>
<div class="padding">
    <h4>Most Popular 10 Place</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@{i++;} @i </span><a href="/Blog/BlogDetail/@x.ID">@x.Title</a></li>

        }

    </ul>
    <a href="/Blog/Index/" class="link1">See all blogs</a>
</div>
